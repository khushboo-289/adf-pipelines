{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-56324"
		},
		"AdvWorksDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDb'"
		},
		"BlobStorageProducts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageProducts'"
		},
		"CusomerDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'CusomerDb'"
		},
		"CustomerBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CustomerBlobStorage'"
		},
		"ProductBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ProductBlobStorage'"
		},
		"AdvWorksDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "khushbooserver7832.database.windows.net"
		},
		"AdvWorksDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DemoDatabase"
		},
		"AdvWorksDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "khushboo"
		},
		"CusomerDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbserver867435.database.windows.net"
		},
		"CusomerDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "exampledb"
		},
		"CusomerDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "khushboo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-56324_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvWorksDb_password": {
						"value": "[parameters('AdvWorksDb_password')]"
					},
					"BlobStorageProducts_connectionString": {
						"value": "[parameters('BlobStorageProducts_connectionString')]"
					},
					"CusomerDb_password": {
						"value": "[parameters('CusomerDb_password')]"
					},
					"CustomerBlobStorage_connectionString": {
						"value": "[parameters('CustomerBlobStorage_connectionString')]"
					},
					"ProductBlobStorage_connectionString": {
						"value": "[parameters('ProductBlobStorage_connectionString')]"
					},
					"AdvWorksDb_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_server')]"
					},
					"AdvWorksDb_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_database')]"
					},
					"AdvWorksDb_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_userName')]"
					},
					"CusomerDb_properties_typeProperties_server": {
						"value": "[parameters('CusomerDb_properties_typeProperties_server')]"
					},
					"CusomerDb_properties_typeProperties_database": {
						"value": "[parameters('CusomerDb_properties_typeProperties_database')]"
					},
					"CusomerDb_properties_typeProperties_userName": {
						"value": "[parameters('CusomerDb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}